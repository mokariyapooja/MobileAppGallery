<!DOCTYPE html>
<html>
<head>
  <title>MobaileAppGallery</title>
  <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track' => true %>
  <%= javascript_include_tag 'application', 'data-turbolinks-track' => true %>
  <%= csrf_meta_tags %>
</head>
<body>
<p class="notice"><%= notice %></p>
<p class="alert"><%= alert %></p>
   <div class="navbar navbar-fixed-top">
    <div class="navbar-inner">
      <div class="container">
        <a class="btn btn-navbar" data-toggle="collapse" data-target=".nav-collapse">
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
          <span class="icon-bar"></span>
        </a>
       <h5>
        <div class="nav-collapse">
          <ul class="nav">
            <li><%= link_to "Home",mobile_apps_path %></li>
            <li><%= link_to "All Mobilr application",mobile_apps_path %></li>
            <li><%= link_to "add mobile app", new_mobile_app_path %> </li>
            <li><%= link_to "Contact Us" ,"#"%></li>
       
            
              <% if admin_signed_in? %>
                <li id="fat-menu" class="dropdown">
                  <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                    <%= current_admin.email %> <b class="caret"></b>
                  </a>
                    <ul class="dropdown-menu">
                      <li><%= link_to "Profile",admins_path %></li>
                      <li><%= link_to "Settings" %></li>
                       <li><%= link_to "Catagory Settings",catagories_path %> </li>
                      <li class="divider"></li>
                      <li>
                       <%= link_to "Sign out", destroy_admin_session_path, :method => :delete %>  </li>
                    </ul>
                </li>
              <% else %>

                <li><%= link_to "Sign up", new_admin_registration_path %> </li> 
                 <li><%= link_to "sign in", new_admin_session_path %></li>
              <% end %>
            
          </ul>
        </div>
      </div>
    </div>
  </div>
</h5>
  <div class="container">
    <div class="row">
      <div class="span9">
        
        <%= gflash %>

        <%= yield %>

        </div>
      <div class="span3">
       
      </div>
    </div>
  </div>    
      
 


</body>
</html>
